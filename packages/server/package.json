{
  "name": "@emasa_monorepo/server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "release": "standard",
    "commit": "cz",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "start:dev": "ts-node-dev --inspect --respawn --transpile-only --ignore-watch node_modules -r tsconfig-paths/register src/shared/infra/index.ts",
    "start:debug": "node start --debug --watch",
    "start:prod": "node dist/index.ts",
    "test": "jest",
    "lint": "eslint --fix",
    "lint2": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@mikro-orm/cli": "^4.2.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.8",
    "@types/dotenv-safe": "^8.1.1",
    "@types/express": "^4.17.8",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/redis": "^2.8.28",
    "@types/request-ip": "^0.0.35",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "commitizen": "^4.2.2",
    "eslint": "^7.12.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^16.0.1",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.2",
    "gitmoji-cli": "^3.2.12",
    "husky": "^4.3.0",
    "prettier": "^2.1.2",
    "standard-version": "^9.0.0",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@mikro-orm/core": "^4.2.3",
    "@mikro-orm/migrations": "^4.2.3",
    "@mikro-orm/mysql": "^4.2.3",
    "@mikro-orm/postgresql": "^4.2.3",
    "@mikro-orm/reflection": "^4.2.3",
    "@types/bluebird": "^3.5.33",
    "assert": "^2.0.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "dotenv-safe": "^8.2.0",
    "envalid": "^6.0.2",
    "express": "^4.17.1",
    "extendable-error": "^0.1.7",
    "helmet": "^4.1.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.4.2",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "tiny-types": "^1.15.0",
    "tsyringe": "^4.3.0",
    "typescript-optional": "^2.0.1",
    "uuid": "^8.3.1"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/config/mikro.config.ts",
      "./dist/config/mikro.config.js"
    ]
  }
}
